{
  "name": "buffer-structures",
  "version": "0.4.1",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/buffer-structures.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "vitest --run",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --outDir lib",
    "bench": "run-s bench:*",
    "bench:hash-map": "ts-node --esm --require tsconfig-paths/register benches/hash-map.ts",
    "bench:hash-set": "ts-node --esm --require tsconfig-paths/register benches/hash-set.ts",
    "bench:hash-map:debug": "NODE_OPTIONS=--inspect-brk ts-node --esm --require tsconfig-paths/register benches/hash-map.ts",
    "bench:hash-set:debug": "NODE_OPTIONS=--inspect-brk ts-node --esm --require tsconfig-paths/register benches/hash-set.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "18",
    "@types/xxhashjs": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "extra-benchmark": "^0.2.3",
    "extra-generator": "^0.5.3",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.1",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.3.4",
    "extra-fsm": "^0.2.1",
    "extra-lazy": "^2.0.2",
    "extra-utils": "^5.5.2",
    "hotypes": "^0.6.4",
    "iterable-operator": "^4.0.6",
    "justypes": "^4.2.1",
    "xxhashjs": "^0.2.2"
  }
}

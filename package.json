  "description": "",
  "keywords": [],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/buffer-structures.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "bench": "ts-node --require tsconfig-paths/register benches/hash-map.ts",
    "bench:debug": "NODE_OPTIONS=--inspect-brk ts-node --require tsconfig-paths/register benches/hash-map.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "extra-benchmark": "^0.2.2",
    "extra-generator": "^0.2.23",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.2.0",
    "@node-rs/xxhash": "^1.2.1",
    "extra-fsm": "^0.1.1",
    "extra-lazy": "^1.3.1",
    "extra-utils": "^4.0.0",
    "hotypes": "^0.5.1",
    "iterable-operator": "^3.1.0",
    "justypes": "^4.1.0"
  }
